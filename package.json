    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.30",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.3.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
